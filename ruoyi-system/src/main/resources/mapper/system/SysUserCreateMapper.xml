<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.SysUserCreateMapper">

	
	<select id="selectUserCreateLimitList" resultType="SysUserCreate" parameterType="Long">
	    select * from sys_user_create where user_id=#{user_id}
	</select>

	
	<insert id="insertUserLimit" parameterType="SysUserCreate">
		insert into sys_user_create(user_id, teacher,student) values

			(#{user_id},#{teacher},#{student})

	</insert>

	<update id="updateUserLimit" parameterType="SysUserCreate" >
		update sys_user_create
<set>
	student=#{student},teacher=#{teacher}
</set>
where user_id=#{user_id}
	</update>
	
</mapper> 